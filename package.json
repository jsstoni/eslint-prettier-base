{
  "name": "format-prettier-base",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "lint": "eslint",
